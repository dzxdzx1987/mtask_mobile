<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".board.view.BoardProgressFragment">


    <com.example.mtask_mobile.CutomView
        android:id="@+id/custom_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:setLeftText="Left Text"
        app:setRightText="Right Text"
        app:setVisible="false">

    </com.example.mtask_mobile.CutomView>

    <ExpandableListView
        android:id="@+id/expand_lv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/custom_view"
        app:layout_constraintLeft_toLeftOf="@+id/custom_view"/>


</androidx.constraintlayout.widget.ConstraintLayout>